{
  "name": "ethereum-store",
  "version": "0.2.0",
  "description": "Ethereum store for react redux",
  "scripts": {
    "prettier": "prettier --write .",
    "test": "react-scripts test --env=./src/utils/testing/jest-environment-jsdom.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll --env=./src/utils/testing/jest-environment-jsdom.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "files": [
    "src/store/ethereum/*",
    "src/config.js",
    "src/package-index.js"
  ],
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "big.js": "^6.2.1",
    "buffer": "^6.0.3",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^9.2.0",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3",
    "reselect": "^4.1.8",
    "web3": "^4.1.1"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "prettier": "^3.0.2",
    "react-scripts": "^5.0.1",
    "sinon": "^15.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ensuro/ethereum-store.git"
  },
  "keywords": [
    "ethereum",
    "insurance",
    "ensuro",
    "redux",
    "store"
  ],
  "author": "Ensuro Dev Team <dev@ensuro.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ensuro/ethereum-store/issues"
  }
}
